<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="moneyText" parent="@android:style/TextAppearance.Medium">
		<item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#111111</item>
        <item name="android:textSize">14dip</item>
        <item name="android:textStyle">normal</item>
        <item name="android:layout_gravity">center_vertical</item>
<!--        <item name="android:typeface">serif</item>-->
	</style>
	<style name="subText" parent="@android:style/TextAppearance.Small">
		<item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#222222</item>
        <item name="android:textStyle">italic</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">marquee</item>
<!--        <item name="android:typeface">serif</item>-->
	</style>
	<style name="subText.Small">
		<item name="android:textSize">12dip</item>
	</style>
    <style name="logoView" parent="@android:style/TextAppearance.Medium">
    	<item name="android:layout_width">wrap_content</item> 
        <item name="android:layout_height">fill_parent</item>
        <item name="android:padding">4dip</item>
<!--        <item name="android:layout_margin">4dip</item>-->
        <item name="android:layout_marginRight">6dip</item>
    </style>
</resources>
