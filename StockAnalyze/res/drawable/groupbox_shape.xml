<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <corners
        android:radius="2dp" />
    <gradient
        android:angle="90"
        android:endColor="#FEFEFF"
        android:startColor="#EBEBF1"
        android:type="linear"/>
    <padding
        android:left="2dp"
        android:top="2dp"
        android:right="2dp"
        android:bottom="2dp" />
    <stroke
        android:width="0.5dp"
        android:color="#909090"/>
</shape>
